                },
                "description": {
                    "@id": "rdfs:comment",
                    "@type": "@id"
                },
                "sort_order": {
                    "@id": "http://www.w3.org/ns/ui#sortPriority",
                    "@type": "@id"
                },
                "exact_match": {
                    "@id": "skos:exactMatch",
                    "@type": "@id"
                },
                "children": {
                    "@id": "skos:narrower",
                    "@type": "skos:Concept",
                    "@container": "@set"
                }
            }
        }
